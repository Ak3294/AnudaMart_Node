    //#endregion Add Coupon

    //#region Edit Coupon
    $(document).on("click", "#edit_btn", function (e) {
        var data = $(this).attr("data-model");
        data = JSON.parse(data);
        const activeStatusId = "<%= activeStatus._id.toString() %>";

       // Populate multi-select for categories, products
    if (data.product_id && data.product_id.length > 0) {
        var productIds = data.product_id.map((product) => product._id);
        $("#edit_product").val(productIds).trigger('change');
    }

        $("#editid").val(data._id);
        $("#edit_product").val(productIds);
        $("#edit_type").val(data.type);
        $("#edit_discount").val(data.discount);
        $("#edit_date_range").val(data.date_range);
        $("#edit_message").val(data.message);
        $("#edit_status_id").prop("checked", data.status_id._id === activeStatusId);
    });

    function getFormData($form) {
        var unindexed_array = $form.serializeArray();
        var indexed_array = {};

        $.map(unindexed_array, function (n, i) {
            indexed_array[n["name"]] = n["value"];
        });

        return indexed_array;
    }

    var editform = $("#editForm");
    editform.on("submit", function (e) {
        e.preventDefault();

        var productIds = $(this).find('select[name="product_id"]').val();

        var formData = getFormData(editform);
        formData.product_id = productIds;

        $.ajax({
            type: "POST",
            url: "/seller/coupon/edit",
            contentType: "application/json",
            data: JSON.stringify(formData),
        })
            .done((res) => {
                if (res.status === 200) {
                    iziToast.success({
                        title: "Success",
                        message: res.message,
                        position: "topRight",
                    });
                    setTimeout(function () {
                        window.location.href = "/seller/coupon/list";
                    }, 1000);
                } else {
                    iziToast.error({
                        title: "Error",
                        message: res.message,
                        position: "topRight",
                    });
                }
            })
            .fail(function (xhr, status, error) {
                iziToast.error({
                    title: "Error",
                    message: xhr.responseText,
                    position: "topRight",
                });
            });
    });
    //#endregion Edit Coupon

    //#region Delete Category
    let couponIdToDelete;
    $(document).ready(function () {
        // Show the modal and store the category ID to delete
        $(".remove-item-btn").click(function () {
            couponIdToDelete = $(this).data("id");
        });

        // AJAX request to delete the category
        $("#delete-record").click(function () {
            $.ajax({
                type: "POST",
                contentType: "application/json",
                url: "/seller/coupon/delete/" + couponIdToDelete,
            })
                .done((res) => {
                    if (res.status == 200) {
                        iziToast.success({
                            title: "Success",
                            message: res.message,
                            position: "topRight",
                        });
                        setTimeout(function () {
                            location.reload();
                        }, 500);
                    } else {
                        iziToast.error({
                            title: "Error",
                            message: res.message,
                            position: "topRight",
                        });
                    }
                })
                .fail(function (xhr, status, error) {
                    iziToast.error({
                        title: "Error",
                        message: xhr.responseText,
                        position: "topRight",
                    });
                });
        });
    });
    //#endregion Delete Category
</script>









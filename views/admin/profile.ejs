<%- include('partials/header') %>

<!--Add Bank modal -->
<div
    class="modal fade"
    id="showModal"
    tabindex="-1"
    aria-labelledby="exampleModalLabel"
    aria-hidden="true"
>
    <div class="modal-dialog modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header px-4 pt-4">
                <h5 class="modal-title" id="exampleModalLabel">
                    Add New Bank Account
                </h5>
                <button
                    type="button"
                    class="btn-close"
                    data-bs-dismiss="modal"
                    aria-label="Close"
                    id="close-modal"
                ></button>
            </div>

            <form class="tablelist-form" novalidate autocomplete="off">
                <div class="modal-body p-4">
                    <div class="mb-3">
                        <label for="bifsc" class="form-label">Bank IFSC</label>
                        <input
                            type="text"
                            id="bifsc"
                            class="form-control"
                            placeholder="Enter IFSC Code"
                        />
                    </div>
                    <div class="mb-3">
                        <label for="bname" class="form-label">Bank Name</label>
                        <input
                            type="text"
                            id="bname"
                            class="form-control"
                            placeholder="Enter Bank Name"
                        />
                    </div>
                    <div class="mb-3">
                        <label for="bholddername" class="form-label"
                            >Account Holder Name</label
                        >
                        <input
                            type="text"
                            id="bholddername"
                            class="form-control"
                            placeholder="Enter Bank Name"
                        />
                    </div>
                    <div>
                        <label for="bacnumber" class="form-label"
                            >Account Number</label
                        >
                        <input
                            type="text"
                            id="bacnumber"
                            class="form-control"
                            placeholder="Enter Bank Name"
                        />
                    </div>
                </div>
                <div class="modal-footer">
                    <div class="hstack gap-2 justify-content-end">
                        <button type="submit" class="btn btn-thm" id="add-btn">
                            Add Bank
                        </button>
                        <button
                            type="button"
                            class="btn btn-ghost-danger"
                            data-bs-dismiss="modal"
                        >
                            <i class="bi bi-x-lg align-baseline me-1"></i> Close
                        </button>
                    </div>
                </div>
            </form>
        </div>
        <!-- modal-content -->
    </div>
    <!-- modal-dialog -->
</div>
